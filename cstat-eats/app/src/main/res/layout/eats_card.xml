<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView_eatsCardPic"
        android:layout_width="232dp"
        android:layout_height="205dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/textView_eatsCardGenres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        android:elevation="2dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsCardName"
        app:srcCompat="@drawable/ic_baseline_image_not_supported_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardAvg5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardAvg4"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardAvg4"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardAvg1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardAvg2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardAvg2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardAvg2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView_eatsCardMy2"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardAvg3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardAvg1"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsCardURL"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardAvg3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardAvg4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardAvg2"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardAvg2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardAvg4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardAvg5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardAvg3"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardAvg3"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardMy5"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardMy4"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardMy4"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardMy1"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:elevation="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardMy2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardMy2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardMy2"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:layout_marginBottom="15dp"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardMy3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardMy1"
        app:layout_constraintTop_toBottomOf="@+id/imageView_eatsCardAvg2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardMy3"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:elevation="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardMy4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardMy2"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardMy2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_eatsCardMy4"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:elevation="2dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardMy5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_eatsCardMy3"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardMy3"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_increaseText"
        android:layout_width="55dp"
        android:layout_height="61dp"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_eatsCardName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_eatsCardName"
        app:layout_constraintTop_toTopOf="@+id/textView_eatsCardName"
        app:srcCompat="@drawable/increase_text_button" />

    <TextView
        android:id="@+id/textView_eatsCardAvgRating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:text="Average Rating:"
        android:textSize="40px"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardAvg1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardAvg1" />

    <TextView
        android:id="@+id/textView_eatsViewMyRating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My Rating:"
        android:textSize="40px"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_eatsCardMy1"
        app:layout_constraintEnd_toStartOf="@+id/imageView_eatsCardMy1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView_eatsCardMy2" />

    <TextView
        android:id="@+id/textView_eatsCardName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="Restaurant Name"

        android:textSize="40px"
        app:layout_constraintBottom_toTopOf="@+id/imageView_eatsCardPic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_eatsCardGenres"
        android:layout_width="252dp"
        android:layout_height="91dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="Genres"
        android:textSize="40px"
        app:layout_constraintBottom_toTopOf="@+id/textView_eatsCardURL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_eatsCardPic" />

    <TextView
        android:id="@+id/textView_eatsCardURL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="URL"
        android:textSize="40px"
        app:layout_constraintBottom_toTopOf="@+id/imageView_eatsCardAvg2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsCardGenres" />

            <TextView
                android:id="@+id/textView_eatsCardOverlay"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:elevation="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>