<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageView
        android:id="@+id/imageView_AvgStar1"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_AvgStar2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsInfoAvgRatingLabel"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_MyStar1"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_MyStar2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsInfoMyRating"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_MyStar2"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_MyStar3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_MyStar1"
        app:layout_constraintTop_toTopOf="@+id/imageView_MyStar1"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_MyStar3"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_MyStar4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_MyStar2"
        app:layout_constraintTop_toTopOf="@+id/imageView_MyStar2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_MyStar4"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_MyStar5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_MyStar3"
        app:layout_constraintTop_toTopOf="@+id/imageView_MyStar1"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_MyStar5"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_MyStar4"
        app:layout_constraintTop_toTopOf="@+id/imageView_MyStar1"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_AvgStar2"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_AvgStar3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_AvgStar1"
        app:layout_constraintTop_toTopOf="@+id/imageView_AvgStar1"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_AvgStar3"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_AvgStar4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_AvgStar2"
        app:layout_constraintTop_toTopOf="@+id/imageView_AvgStar2"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_AvgStar4"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_AvgStar5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_AvgStar3"
        app:layout_constraintTop_toTopOf="@+id/imageView_AvgStar3"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <ImageView
        android:id="@+id/imageView_AvgStar5"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_AvgStar4"
        app:layout_constraintTop_toTopOf="@+id/imageView_AvgStar4"
        app:srcCompat="@drawable/ic_baseline_star_border_24" />

    <TextView
        android:id="@+id/textView_eatsInfoURL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="URL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_eatsInfoName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Name"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_eatsInfoGenres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <TextView
        android:id="@+id/textView_eatsInfoGenres"
        android:layout_width="248dp"
        android:layout_height="85dp"
        android:gravity="center"
        android:text="Genres"
        app:layout_constraintBottom_toTopOf="@+id/textView_eatsInfoURL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsInfoName" />

    <TextView
        android:id="@+id/textView_eatsInfoAvgRatingLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="35dp"
        android:text="Average Rating:"
        app:layout_constraintBottom_toTopOf="@+id/imageView_AvgStar1"
        app:layout_constraintEnd_toEndOf="@+id/imageView_AvgStar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_eatsInfoURL" />

    <TextView
        android:id="@+id/textView_eatsInfoMyRating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="25dp"
        android:text="My Rating:"
        app:layout_constraintEnd_toEndOf="@+id/imageView_MyStar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_AvgStar1" />

</androidx.constraintlayout.widget.ConstraintLayout>